  "scripts": {
    "build:webpack": "cross-env NODE_ENV=production webpack --mode=production",
    "deploy": "npm i && npm run build:webpack && NODE_ENV=production DEPLOY=true node ./scripts/build.js",
    "build-prod": "npm i && npm run build:webpack && NODE_ENV=production node ./scripts/build.js && node ./scripts/mac-zip.js",
    "build-dev": "npm i && node ./scripts/build-setup.js && npm dedupe && npm run build:webpack && NODE_ENV=development node ./scripts/build.js && node ./scripts/mac-zip.js",
    "build-local": "npm i && node ./scripts/build-setup.js && npm dedupe && npm run build:webpack && NODE_ENV=local node ./scripts/build.js && node ./scripts/mac-zip.js",
    "start": "electron . --log-level=debug",
    "build-native": "node ./scripts/build-native.js",
    "clean": "node scripts/clean.js",
    "clean:db": "node scripts/clean-db.js",
    "docs": "mkdir -p docs && jsdoc -t ./node_modules/minami -d docs -R README.md -r src/",
    "test": "nyc --reporter=lcov --check-coverage --lines=1 --functions=1 --branches=1 babel-node test/",
    "test:seed-dexie": "node ./test/data/seed-collection-data.js",
    "test:e2e-single": "npm run build:webpack && env NODE_ENV=test mocha --timeout=120000 test/e2e/computation-run.test.js",
    "test:e2e": "npm run build:webpack && env NODE_ENV=test mocha --timeout=120000 test/e2e/*.test.js",
    "watch": "cross-env NODE_ENV=development webpack-dev-server --mode=development"
  },
