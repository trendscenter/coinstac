{
  "meta": {
    "name": "Decentralized Parallel Independent Component Analysis demo",
    "id": "coinstac-dpica",
    "version": "v1.0.0",
    "repository": "https://github.com/MartOfTheNorth/dpica/tree/simulator",
    "description": "Decentralized Parallel Independent Component Analysis on FIT Data",
    "compspecVersion": 1
  },
  "computation": {
    "type": "docker",
    "dockerImage": "coinstacteam/coinstac-dpica",
    "command": [
      "python",
      "\/computation\/scripts\/local_pipeline.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "coinstacteam/coinstac-dpica",
      "command": [
        "python",
        "\/computation\/scripts\/remote_pipeline.py"
      ]
    },
    "input":{
      "data": {
        "label": "File Path",
        "type": "directory",
        "items": ["File Path"],
        "default": "",
        "source": "member",
        "order": 0
      },
      "lambda":{
        "label":"Lambda",
        "type":"number",
        "default":1,
        "order":1,
        "source": "owner"
      }
   },
   "output": {},
   "display": {
     "type": "string"
   }
 }
}
