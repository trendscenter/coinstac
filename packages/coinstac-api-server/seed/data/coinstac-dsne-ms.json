{
  "meta": {
    "name": "Decentralized Multishot TSNE Demo",
    "id": "dsne-ms",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/trendscenter\/coinstac-dsne-multishot",
    "description": "A Test from Decentralized Multishot TSNE",
    "testData": "tree\/master\/test\/input"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "coinstacteam/dsne-ms",
    "command": [
      "python",
      "\/computation\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "coinstacteam/dsne-ms",
      "command": [
        "python",
        "\/computation\/remote.py"
      ]
    },
    "input": {
      "site_data": {
        "label": "Site Data",
        "type": "files",
        "items": ["site_data"],
        "extensions": [["csv", "txt"]],
        "source": "member"
      },
      "site_label": {
        "label": "Site Label",
        "type": "files",
        "items": ["site_label"],
        "extensions": [["csv", "txt"]],
        "source": "member"
      },
      "no_dims": {
        "type": "number",
        "label": "No DIMS",
        "default": 2,
        "order": 2,
        "source": "owner"
      },
      "initial_dims": {
        "type": "number",
        "label": "Initial DIMS",
        "default": 30,
        "order": 3,
        "source": "owner"
      },
      "perplexity": {
        "type": "range",
        "label": "Perplexity",
        "default": 100,
        "min": -5,
        "max": 100,
        "step": 5,
        "order": 4,
        "source": "owner"
      },
      "max_iterations": {
        "type": "number",
        "label": "Max Iterations",
        "default": 1000,
        "order": 5,
        "source": "owner"
      }
    },
    "output": {
      "embedding": {
        "type": "array"
      },
      "image_path": {
        "type": "string"
      }
    },
    "display": {
      "type": "images"
    }
  }
}
