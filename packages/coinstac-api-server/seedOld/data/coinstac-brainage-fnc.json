{
  "meta": {
    "name": "Brainage prediction using Support Vector Regression Demo",
    "id": "dpsvr-fnc",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/trendscenter\/coinstac-brainage-fnc",
    "description": "Brain Age Prediction using Linear SVR model and FNC matrices as features\n\n"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "coinstacteam/dpsvr-fnc",
    "command": [
      "python",
      "\/computation\/scripts\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "coinstacteam/dpsvr-fnc",
      "command": [
        "python",
        "\/computation\/scripts\/remote.py"
      ]
    },
    "input": {
      "site_data": {
        "label": "Site Data",
        "type": "files",
        "items": ["site_data"],
        "extensions": [["csv", "txt"]],
        "source": "member"
      },
      "site_label": {
        "label": "Site Label",
        "type": "files",
        "items": ["site_label"],
        "extensions": [["csv", "txt"]],
        "source": "member"
      },
      "input_source":
      {
        "default": "GICA",
        "label": "Input Source",
        "type": "string",
        "source": "owner",
        "order": 2
      },
      "split_type":
      {
        "default": "random",
        "label": "Split Type",
        "type": "string",
        "source": "owner",
        "order": 3
      },
      "epsilon_local":
      {
        "default": 0.0,
        "label": "Epsilon Local",
        "type": "number",
        "source": "owner",
        "order": 4
      },
      "fit_intercept_local":
      {
        "default": true,
        "label": "Fit Intercept Local",
        "type": "boolean",
        "source": "owner",
        "order": 5
      },
      "intercept_scaling_local":
      {
        "default": 1.0,
        "label": "Intercept Scaling Local",
        "type": "number",
        "source": "owner",
        "order": 6
      },
      "tolerance_local":
      {
        "default": 0.0001,
        "label": "Tolerance Local",
        "type": "number",
        "source": "owner",
        "order": 7
      },
      "regularization_local":
      {
        "default": 1,
        "label": "Regularization Local",
        "type": "number",
        "source": "owner",
        "order": 8
      },
      "loss_local":
      {
        "default": "epsilon_insensitive",
        "label": "Loss Local",
        "type": "string",
        "source": "owner",
        "order": 9
      },
      "dual_local":
      {
        "default": true,
        "label": "Dual Local",
        "type": "boolean",
        "source": "owner",
        "order": 10
      },
      "random_state_local":
      {
        "default": 0,
        "label": "Random State Local",
        "type": "number",
        "source": "owner",
        "order": 11
      },
      "max_iterations_local":
      {
        "default": 10000,
        "label": "Max Iterations Local",
        "type": "number",
        "source": "owner",
        "order": 12
      },
      "epsilon_owner":
      {
        "default": 0.0,
        "label": "Espilon Owner",
        "type": "number",
        "source": "owner",
        "order": 13
      },
      "fit_intercept_owner":
      {
        "default": true,
        "label": "Fit Intercept Owner",
        "type": "boolean",
        "source": "owner",
        "order": 14
      },
      "intercept_scaling_owner":
      {
        "default": 1.0,
        "label": "Intercept Scaling Owner",
        "type": "number",
        "source": "owner",
        "order": 15
      },
      "tolerance_owner":
      {
        "default": 0.0001,
        "label": "Tolerance Owner",
        "type": "number",
        "source": "owner",
        "order": 16
      },
      "regularization_owner":
      {
        "default": 1,
        "label": "Regularization Owner",
        "type": "number",
        "source": "owner",
        "order": 17
      },
      "loss_owner":
      {
        "default": "epsilon_insensitive",
        "label": "Loss Owner",
        "type": "boolean",
        "source": "owner",
        "order": 18
      },
      "dual_owner":
      {
        "default": true,
        "label": "Dual Owner",
        "type": "boolean",
        "source": "owner",
        "order": 19
      },
      "random_state_owner":
      {
        "default": 0,
        "label": "Random State Owner",
        "type": "number",
        "source": "owner",
        "order": 20
      },
      "max_iterations_owner":
      {
        "default": 10000,
        "label": "Max Iterations Owner",
        "type": "number",
        "source": "owner",
        "order": 21
      },
      "test_size":
      {
        "default": 0.1,
        "label": "Test Size",
        "type": "number",
        "source": "owner",
        "order": 22
      },
      "shuffle":
      {
        "default": true,
        "label": "Dual Owner",
        "type": "boolean",
        "source": "owner",
        "order": 23
      }
    },
    "output": {
      "regressions": {
        "label": "Regressions",
        "type": "table",
        "items": {
          "global_stats": {
            "label": "Global Stats",
            "type": "object",
            "items": {
              "avg_beta_vector": {
                "label": "Avg Beta Vector",
                "type": "array"
              },
              "r2_global": {
                "label": "R2 Global",
                "type": "number"
              },
              "ts_global": {
                "label": "TS Global",
                "type": "array"
              },
              "ps_global": {
                "label": "PS Global",
                "type": "number"
              },
              "dof_global": {
                "label": "DOF Global",
                "type": "array"
              }
            }
          },
          "local_stats": {
            "label": "Local Stats",
            "type": "object",
            "items": {
              "beta": {
                "label": "Beta",
                "type": "array"
              },
              "pval": {
                "label": "PVal",
                "type": "array"
              },
              "rsquared": {
                "label": "R Squared",
                "type": "number"
              },
              "sse": {
                "label": "SSE",
                "type": "number"
              },
              "tval": {
                "label": "TVal",
                "type": "array"
              }
            }
          },
          "ROI": {
            "label": "Region of Interest",
            "type": "string"
          }
        }
      }
    },
    "display": [
      {
        "type": "string",
        "tables": [
          {
            "source": "regressions",
            "subtables": [
              {
                "source": "global_stats",
                "subtitle": "ROI"
              },
              {
                "source": "local_stats",
                "subtitle": "ROI",
                "subtables": "by-key"
              }
            ]
          }
        ]
      }
    ]
  }
}
