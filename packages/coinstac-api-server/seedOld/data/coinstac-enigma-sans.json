{
  "meta": {
    "name": "ENIGMA in COINSTAC demo",
    "id": "enigma-sans",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/trendscenter\/coinstac-enigma-sans",
    "description": "A test for decentralized PCA"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "coinstacteam/enigma-sans",
    "command": [
      "python",
      "\/computation\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "coinstacteam/enigma-sans",
      "command": [
        "python",
        "\/computation\/remote.py"
      ]
    },
    "input": {
      "data": {
        "label": "Data",
        "type": "files",
        "items": ["ENIGMA"],
        "extensions": [["csv","txt"]],
        "order": 0,
        "source": "member"
      }
    },
    "output": {
      "PC_global": {
        "type": "array"
      },
      "projM_global": {
        "type" : "array"
      },
      "bkprojM_global" : {
        "type" : "array"
      }
    }
  }
}
